{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/*.Tests.csproj",
            "**/Moongate.Server.Metrics.Generators.csproj",
            "**/Moongate.Network.Packets.Generators.csproj",
            "**/Moongate.Abstractions/Moongate.Abstractions.csproj",
            "**/Moongate.Core/Moongate.Core.csproj",
            "**/Moongate.Scripting/Moongate.Scripting.csproj"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "allowCompilationErrors": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Moongate v2",
      "_appTitle": "Moongate v2 Documentation",
      "_appFooter": "Moongate v2 - Ultima Online Server Emulator. Licensed under GPL-3.0.",
      "_appLogoPath": "images/moongate_logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "pdf": false
    },
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
